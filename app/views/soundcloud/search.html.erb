
<div class="panel-heading">
  <%= form_tag(search_soundcloud_path, method: :get) do %>
    <%= text_field_tag :query, params[:query] %>
    <%= submit_tag "Search", class: "btn btn-default" %>
  <% end %>
</div>

<br/>

<div class="col-md-10 col-xs-12 col-sm-9">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <%= render partial: "soundcloud_user_table", locals: {soundcloud_users: @soundcloud_users} %>
      </div>
    </div>
  </div>
</div>
